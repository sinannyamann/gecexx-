{
  "name": "ultra-advanced-agi-agent",
  "version": "4.0.0",
  "description": "Ultra Advanced Self-Evolving AGI Agent with real personality, learning capabilities, and advanced tools",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "build": "echo 'No build step required for this project'",
    "test": "node --test test/*.js",
    "lint": "eslint index.js --fix",
    "format": "prettier --write index.js",
    "health": "curl -f http://localhost:${PORT:-5000}/health || exit 1",
    "logs": "tail -f logs/combined.log",
    "clean": "rm -rf logs/* temp/* uploads/*",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "railway:status": "railway status",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "cache:clear": "node scripts/clear-cache.js",
    "performance": "node scripts/performance-test.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "pg": "^8.11.3",
    "openai": "^4.24.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "node-cache": "^5.1.2",
    "express-session": "^1.17.3",
    "dotenv": "^16.3.1",
    "cheerio": "^1.0.0-rc.12",
    "nodemailer": "^6.9.7",
    "node-cron": "^3.0.3",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "boxen": "^7.1.1",
    "gradient-string": "^2.0.2",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "keywords": [
    "agi",
    "artificial-intelligence",
    "chatbot",
    "ai-agent",
    "machine-learning",
    "neural-network",
    "natural-language-processing",
    "conversational-ai",
    "self-evolving",
    "railway",
    "nodejs",
    "express",
    "websocket",
    "postgresql",
    "redis",
    "openai",
    "anthropic",
    "deepseek",
    "turkish-ai",
    "personality-ai"
  ],
  "author": {
    "name": "Ultra AGI Developer",
    "email": "developer@ultraagi.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ultra-advanced-agi-agent.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ultra-advanced-agi-agent/issues"
  },
  "homepage": "https://github.com/yourusername/ultra-advanced-agi-agent#readme",
  "railway": {
    "build": {
      "command": "npm install"
    },
    "start": {
      "command": "npm start"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-undef": "error"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "index.js",
      "lib/",
      "config/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "logs/",
      "temp/",
      "uploads/",
      "coverage/"
    ],
    "delay": 1000
  }
}
